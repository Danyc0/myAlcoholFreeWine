<h1>Listing Wines</h1>
 
<table>
   <tr>
       <th>Name</th>
       <th>Price</th>
   </tr>
   <% @wines.each do |wine| %>
      <tr>
           <td><%= wine.name %></td>
           <td><%= wine.price %></td>
      </tr>
  <% end %>
</table>

<div class="digg_pagination">
    <div>
        <%= page_entries_info @users %>
    </div>
    <%= will_paginate @users, container: false %>
</div>
